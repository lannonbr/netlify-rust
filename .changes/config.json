{
  "gitSiteUrl": "https://github.com/lannonbr/netlify_deploy/",
  "pkgManagers": {
    "rust": {
      "version": true,
      "getPublishedVersion": "git log v${ pkgFile.version } -1 --pretty=%Cgreen${ pkgFile.version } || echo \"not published yet\"",
      "publish": "echo boop"
    }
  },
  "packages": {
    "netlify_deploy": {
      "path": ".",
      "manager": "rust",
      "releaseTag": "v${ pkgFile.version }"
    }
  }
}
